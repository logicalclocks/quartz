{
  "homepage": "https://github.com/logicalclocks/quartz",
  "name": "@logicalclocks/quartz",
  "version": "2.13.3",
  "description": "Logical Clocks Design System Library",
  "author": "logicalclocks",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/logicalclocks/quartz.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./chakra": {
      "import": "./dist/chakra.mjs",
      "types": "./dist/chakra.d.ts"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "vite build",
    "start": "vite build --watch",
    "prepack": "pnpm run build",
    "gen:chakra-theme-typings": "chakra-cli tokens src/theme-chakra/theme/index.ts",
    "test:lint": "eslint --ext .ts,.tsx ./src",
    "test:prettier": "prettier --check ./src",
    "prettier": "prettier --write ./src",
    "deploy": "gh-pages -d storybook-static",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "report-test": "playwright show-report",
    "chromatic": "npx chromatic --project-token dqq8k95raoo",
    "semantic-release": "semantic-release",
    "commit": "cz",
    "publish-local": "yalc publish --push",
    "push-local": "yalc push",
    "serve:storybook:test": "pnpm storybook & pnpm wait-on http://localhost:6006 && pnpm run test-storybook"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.2.2",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/styled-system": "^2.9.2",
    "@codemirror/autocomplete": "^6.11.0",
    "@codemirror/view": "^6.22.0",
    "@emotion/core": "^10.3.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.10.6",
    "@rebass/forms": "^4.0.6",
    "@styled-system/css": "^5.1.5",
    "@tanem/react-nprogress": "^5.0.51",
    "@tanstack/react-table": "^8.10.7",
    "@types/testing-library__jest-dom": "^5.14.7",
    "@uiw/codemirror-extensions-langs": "^4.21.20",
    "@uiw/codemirror-theme-darcula": "^4.21.20",
    "@uiw/react-codemirror": "^4.21.20",
    "chakra-react-select": "^4.7.6",
    "date-fns": "^2.30.0",
    "emotion-theming": "^10.3.0",
    "ramda": "^0.28.0",
    "react-datepicker": "^4.23.0",
    "rebass": "^4.0.7"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@playwright/experimental-ct-react": "^1.40.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.4",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.3",
    "@semantic-release/npm": "^10.0.5",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-docs": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/core-common": "7.5.3",
    "@storybook/csf-tools": "7.5.3",
    "@storybook/instrumenter": "7.5.3",
    "@storybook/jest": "^0.2.3",
    "@storybook/node-logger": "7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/test-runner": "^0.15.2",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.5.3",
    "@storybook/types": "7.5.3",
    "@types/node": "^16.18.38",
    "@types/ramda": "^0.28.1",
    "@types/react": "^18.2.38",
    "@types/react-datepicker": "^4.19.3",
    "@types/react-dom": "^18.2.16",
    "@types/rebass": "^4.0.14",
    "@types/rebass__forms": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.2.0",
    "chromatic": "^10.9.6",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "jest": "29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.19.0",
    "semantic-release": "21.0.1",
    "storybook": "^7.5.3",
    "storybook-dark-mode": "^3.0.1",
    "typescript": "^5.3.2",
    "vite": "^4.5.2",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.6.3",
    "vite-plugin-svgr": "^4.2.0",
    "wait-on": "^7.2.0",
    "yalc": "^1.0.0-pre.53"
  },
  "files": [
    "dist",
    ".releaserc.json"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "packageManager": "pnpm@8.6.6",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "volta": {
    "node": "20.9.0"
  },
  "pnpm": {
    "overrides": {
      "semver@<5.7.2": ">=5.7.2"
    }
  }
}